+ SlashSpecifier 1.0 .
SLASH_ROOT:=
NO_SLASH:=no-slash
BACK_SLASH:=back\slash\paths
FORWARD_SLASH:=forward/slash/paths
MIXED_SLASH:=forward/back\paths
APPENDED_SLASH+:=forward/back\paths

+ BackSlashModuleSpecifier 1.0 .\back\module
BACK_SLASH_ROOT:=
BACK_NO_SLASH:=no-slash
BACK_BACK_SLASH:=back\slash\paths
BACK_FORWARD_SLASH:=forward/slash/paths
BACK_MIXED_SLASH:=forward/back\paths
APPENDED_SLASH+:=forward/back\paths

+ ForeSlashModuleSpecifier 1.0 .\fore\module
FORE_SLASH_ROOT:=
FORE_NO_SLASH:=no-slash
FORE_BACK_SLASH:=back\slash\paths
FORE_FORWARD_SLASH:=forward/slash/paths
FORE_MIXED_SLASH:=forward/back\paths
APPENDED_SLASH+:=forward/back\paths
